{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        },
        {
            "src": "views/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/index.js"
        },
        {
            "src": "/views/(.*)",
            "dest": "/views/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ]
}
