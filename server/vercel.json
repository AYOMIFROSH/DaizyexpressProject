{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/index.js",
            "maxDuration": 30
        },
        {
            "src": "/(.*)",
            "dest": "/index.js",
            "maxDuration": 30
        }
    ],
    "functions": {
        "api/*": {
            "memory": 1024,
            "maxDuration": 30
        }
    }
}